{
  "presets": ["react"],
  "plugins": ["transform-es2015-modules-commonjs", "add-module-exports"],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      "presets": ["react-optimize"],
      "plugins": ["babel-plugin-dev-expression"]
    }
  }
}
