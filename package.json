{
  "name": "Galeri",
  "productName": "Galeri",
  "version": "0.0.14",
  "author": "Micheal Parks",
  "description": "Fantastic Art Rendering Technology",
  "main": "main.js",
  "private": true,
  "scripts": {
    "test": "depcheck",
    "start:dev": "cross-env NODE_ENV=development electron ./main.development",
    "start": "cross-env NODE_ENV=production electron ./",
    "webpack:dev": "webpack --config webpack.config.development.js --watch --progress --profile --colors",
    "dev": "concurrently --kill-others \"npm run start:dev\" \"npm run webpack:dev\"",
    "dev:prod": "concurrently --kill-others \"cross-env HOT=1 NODE_ENV=production electron ./main.development\" \"cross-env NODE_ENV=production node server\"",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.config.electron.js --progress --profile --colors",
    "build:renderers": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build:main & npm run build:renderers",
    "package": "cross-env NODE_ENV=production node package.js",
    "package:mac": "node ./bin/package.js darwin --sign",
    "analytics": "open -a \"Google Chrome\" bin/analytics.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/galeri.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michealparks/galeri/issues"
  },
  "homepage": "https://github.com/michealparks/galeri#readme",
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.4.5"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babili-webpack-plugin": "0.0.7",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "cross-zip": "^2.1.5",
    "del": "^2.2.2",
    "depcheck": "^0.6.4",
    "devtron": "^1.4.0",
    "electron": "^1.4.6",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-osx-sign": "^0.4.1",
    "electron-packager": "^8.2.0",
    "electron-winstaller": "^2.4.1",
    "express": "^4.14.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "nobin-debian-installer": "0.0.10",
    "run-series": "^1.1.4",
    "webpack": "^1.13.3",
    "webpack-merge": "^0.15.0",
    "webpack-validator": "^2.2.9"
  },
  "dependencies": {
    "auto-launch": "5.0.1"
  },
  "build": {
    "appId": "caboodle.by.micheal.parks",
    "category": "public.app-category.lifestyle",
    "copyright": "Copyright Â© 2016 Micheal Parks"
  }
}
