{
  "name": "Galeri",
  "productName": "Galeri",
  "version": "0.0.23",
  "author": "Micheal Parks",
  "description": "F.A.R.T. - Fantastic Art Rendering Technology",
  "main": "main.js",
  "private": true,
  "scripts": {
    "test": "depcheck",
    "start:dev": "cross-env NODE_ENV=development electron ./main.development",
    "start": "cross-env NODE_ENV=production electron ./",
    "webpack:dev": "node webpack.js dev",
    "dev": "concurrently --kill-others \"npm run start:dev\" \"node ./bin/stylus dev\" \"npm run webpack:dev\"",
    "dev:prod": "concurrently --kill-others \"cross-env NODE_ENV=production electron ./main.development\" \"node ./bin/stylus dev\"",
    "build": "node webpack.js",
    "package": "cross-env NODE_ENV=production node package.js",
    "package:mac": "node webpack.js && node ./bin/package.js darwin --sign",
    "package:win": "node webpack.js && node ./bin/package.js win32",
    "analytics": "open -a \"Google Chrome\" bin/analytics.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/galeri.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michealparks/galeri/issues"
  },
  "homepage": "https://github.com/michealparks/galeri#readme",
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.4.5"
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babili-webpack-plugin": "0.0.11",
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "cross-zip": "^2.1.5",
    "del": "^2.2.2",
    "depcheck": "^0.6.7",
    "devtron": "^1.4.0",
    "electron": "^1.6.6",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-osx-sign": "^0.4.4",
    "electron-packager": "^8.7.0",
    "electron-winstaller": "^2.5.2",
    "html-minifier": "^3.4.3",
    "inline-source": "^5.2.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "nobin-debian-installer": "0.0.10",
    "run-series": "^1.1.4",
    "stylus": "^0.54.5",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "auto-launch": "5.0.1",
    "open": "0.0.5"
  },
  "build": {
    "appId": "galeri.by.micheal.parks",
    "category": "public.app-category.lifestyle",
    "copyright": "Copyright Â© 2016 Micheal Parks"
  },
  "standard": {
    "globals": [
      "__VERSION__",
      "__dev__"
    ]
  }
}
